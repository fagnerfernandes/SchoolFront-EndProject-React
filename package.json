  "scripts": {
    "start": "react-scripts start",
    "build:clean": "rimraf build",
    "build": "npm run build:clean && REACT_APP_ENV=production react-scripts build",
    "deploy:staging": "sh scripts/deploy-staging.sh",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint 'src/**' --fix",
